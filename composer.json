{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "abdullahobaid/mobilywslaraval": "dev-master",
        "brozot/laravel-fcm": "1.2.*",
        "geckob/firebase": "^1.0",
        "kreait/firebase-php": "4.0",
        "laravel/framework": "5.4.*",
        "laravel/passport": "4.0.3",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "laravelrus/localized-carbon": "2.2",
        "mpociot/laravel-firebase-sync": "~1.0",
        "phpoffice/phpexcel": "^1.8",
        "pusher/pusher-php-server": "^3.0",
        "symfony/css-selector": "4.0.3",
        "vzool/malath_sms": "^0.04.0",
        "yajra/laravel-datatables-oracle": "8.3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/system.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
